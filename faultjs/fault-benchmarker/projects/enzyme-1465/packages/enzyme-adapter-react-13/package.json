    "lodash": "^4.17.4",
    "object.assign": "^4.1.0",
    "object.values": "^1.0.4",
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "enzyme": "^3.0.0",
    "react": "^0.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "enzyme": "^3.0.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "in-publish": "^2.0.0",
    "rimraf": "^2.6.2"
