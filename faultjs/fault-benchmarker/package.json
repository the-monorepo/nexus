{
  "name": "@fault/benchmarker",
  "version": "1.0.0-alpha.0",
  "description": "Also known as DefectsJS. A benchmarking library for measuring various fault localization/repair algorithms in Fault.js",
  "types": "./src/index.ts",
  "module": "./esm/index.js",
  "main": "./lib/index.js",
  "scripts": {
    "start": "node --max-old-space-size=4096 ./lib/index.js",
    "results": "node ./lib/results.js",
    "build": "yarn start && yarn results",
    "report": "webpack-dev-server --watch --mode development",
    "scrape-repo": "node ./lib/scrape-repo.js"
  },
  "private": false,
  "files": [
    "lib",
    "src",
    "esm"
  ],
  "license": "MIT",
  "author": "Patrick Shaw <mail@patrickshaw.me>",
  "repository": {
    "type": "git",
    "url": "https://github.com/PatrickShaw/monorepo.git",
    "directory": "faultjs/fault-ast-localization"
  },
  "keywords": [
    "testing",
    "test",
    "typescript",
    "faultjs",
    "fault-localization",
    "debugging",
    "benchmark"
  ],
  "dependencies": {
    "@babel/parser": "^7.5.0",
    "@fault/addon-sbfl": "1.0.0-alpha.0",
    "@fault/record-faults": "1.0.0-alpha.0",
    "@fault/runner": "1.0.0-alpha.0",
    "@fault/sbfl-barinel": "1.0.0-alpha.0",
    "@fault/sbfl-dstar": "1.0.0-alpha.0",
    "@fault/sbfl-op2": "1.0.0-alpha.0",
    "@fault/sbfl-tarantula": "1.0.0-alpha.0",
    "@fault/tester-mocha": "1.0.0-alpha.0",
    "micromatch": "^4.0.2"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^4.4.0",
    "cross-spawn": "^6.0.5",
    "isomorphic-git": "^0.58.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
