{
  "name": "by-example",
  "description": "Extracts the types used in a particular object",
  "workspaces": [
    "packages/*",
    "build-packages/*"
  ],
  "private": true,
  "author": "Patrick Shaw <mail@patrickshaw.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PatrickShaw/by-example.git"
  },
  "scripts": {
    "format": "prettier --write \"{**/,}*.{js,jsx,ts,tsx}\"",
    "test": "jest --coverage",
    "build": "gulp build",
    "watch": "gulp watch"
  },
  "keywords": [
    "types"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-typescript": "^7.0.0-beta.40",
    "@types/jest": "^22.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.2.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "colors": "^1.3.1",
    "gulp": "4",
    "gulp-babel": "8.0.0-beta.2",
    "gulp-newer": "^1.4.0",
    "gulp-watch": "^5.0.1",
    "husky": "1.0.0-rc.13",
    "jest": "^22.3.0",
    "lerna": "^3.0.0-beta.21",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.2",
    "through2": "^2.0.3",
    "typescript": "^2.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn test"
    }
  }
}
