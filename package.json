{
  "name": "css-color-checker",
  "version": "1.0.1",
  "description": "Library for whether a value is a valid color value",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src --out-dir build --extensions .tsx",
    "format": "prettier --write {**/,}/*.{js,jsx,ts,tsx}",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PatrickShaw/css-color-checker.git"
  },
  "keywords": [
    "css",
    "color"
  ],
  "author": "Patrick Shaw",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "run-s format test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-typescript": "^7.0.0-beta.40",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.2.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "husky": "^0.15.0-rc.9",
    "jest": "^22.3.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "css-color-names": "^0.0.4"
  }
}
