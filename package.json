{
  "name": "css-color-checker",
  "version": "1.0.2",
  "description": "Library for checking whether a value is a valid color value",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/PatrickShaw/css-color-checker.git"
  },
  "keywords": [
    "css",
    "color",
    "type",
    "check",
    "util",
    "helper",
    "string",
    "pattern",
    "typescript",
    "regex"
  ],
  "scripts": {
    "format": "prettier --write \"{**/,}*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "ci-test": "yarn test && codecov",
    "build": "babel src --out-dir lib --extensions \".ts\"",
    "prepublish": "yarn build",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.54",
    "@babel/core": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/preset-typescript": "7.0.0-beta.54",
    "@types/jest": "^23.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "husky": "^0.15.0-rc.9",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.11.1",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "css-color-names": "^0.0.4"
  },
  "engines": {
    "node": ">=8.11.3"
  }
}
