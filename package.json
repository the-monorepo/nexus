{
  "name": "jest-mock-functions",
  "description": "Mock all functions inside an array or object literal",
  "version": "3.0.1",
  "main": "./lib/index.js",
  "types": "./src/index.ts",
  "files": [
    "lib",
    "src"
  ],
  "license": "MIT",
  "author": "Patrick Shaw <mail@patrickshaw.me>",
  "repository": "https://github.com/PatrickShaw/jest-mock-functions",
  "keywords": [
    "jest",
    "mock",
    "testing",
    "test",
    "typescript",
    "easy",
    "util",
    "helper",
    "function"
  ],
  "scripts": {
    "format": "prettier --write \"{**/,}*.{js,jsx,ts,tsx}\"",
    "test": "yarn test:types && yarn test:jest",
    "test:types": "tsc --noEmit",
    "test:jest": "jest",
    "ci-test": "yarn test && codecov",
    "build": "babel src --out-dir lib --extensions \".ts\"",
    "prepublish": "yarn build",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precommit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/jest": "^23.3.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/node": "^10.9.4"
  },
  "engines": {
    "node": ">=6"
  }
}
