{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "@pshaw/eslint-config-core",
    "@pshaw/eslint-config-react",
    "@pshaw/eslint-config-typescript",
    "prettier"
  ],
  "env": {
      "browser": true,
      "amd": true,
      "node": true,
      "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "rules": {
    "react/prop-types": "off"
  }
}