{
  "editor.tabSize": 2,
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "search.exclude": {
    ".pnp.{mjs,cjs,js}": true,
    ".yarn/**": true,
    "yarn-error.log": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "stylelint.stylelintPath": ".yarn/sdks/stylelint/lib/index.js",
  "files.associations": {
    "array": "cpp",
    "iterator": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "vector": "cpp",
    "__bit_reference": "cpp",
    "__hash_table": "cpp",
    "__split_buffer": "cpp",
    "__tree": "cpp",
    "deque": "cpp",
    "initializer_list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "unordered_map": "cpp"
  }
}
